{
	"angularCompilerOptions": {
		"entryModule": "app/app.module#AppModule",
		"preserveWhitespaces": false,
		"enableLegacyTemplate": true,
		"jitDevMode": true,
		"legacy_enabled": true,
		"annotationsAs": "decorators"
	},
	"compilerOptions": {
		"baseUrl": ".",
		"paths": {
			"store": [
				"src/store.ts"
			]
		},
		"emitDecoratorMetadata": true,
		"experimentalDecorators": true,
		"importHelpers": true,
		"lib": [
			"dom",
			"es2016"
		],
		"module": "es2015",
		"moduleResolution": "node",
		"noEmitHelpers": true,
		"noImplicitAny": true,
		"removeComments": true,
		"rootDir": ".",
		"sourceMap": true,
		"skipLibCheck": true,
		"target": "es5"
	}
}