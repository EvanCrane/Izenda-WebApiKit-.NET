{
	"name": "izenda-angular2-starter-kit",
	"version": "1.0.0",
	"repository": {
		"type": "git",
		"url": "https://github.com/Izenda7Series/Angular2Starterkit.git"
	},
	"scripts": {
		"start": "npm run build:dev",
		"build": "cross-env NODE_ENV=production webpack -p",
		"clean": "rimraf build",
		"build:dev": "cross-env NODE_ENV=development webpack-dev-server --inline --hot",
		"build:production": "npm run clean && npm run build",
		"test": "jest",
		"test:watch": "jest --watch",
		"test:cov": "jest --coverage"
	},
	"jest": {
		"preset": "jest-preset-angular",
		"roots": [
			"<rootDir>/src/"
		],
		"setupTestFrameworkScriptFile": "<rootDir>/src/jest.config.ts",
		"moduleNameMapper": {
			"store": "<rootDir>/src/store.ts"
		}
	},
	"license": "MIT",
	"dependencies": {
		"@angular/animations": "^5.2.0",
		"@angular/common": "^5.2.0",
		"@angular/core": "^5.2.0",
		"@angular/forms": "^5.2.0",
		"@angular/http": "^5.2.0",
		"@angular/platform-browser": "^5.2.0",
		"@angular/platform-browser-dynamic": "^5.2.0",
		"@angular/router": "^5.2.0",
		"@types/jest": "^22.1.1",
		"bootstrap": "^4.1.3",
		"core-js": "^2.5.3",
		"font-awesome": "^4.7.0",
		"jest": "^22.1.4",
		"jest-preset-angular": "^5.0.0",
		"jquery": "^3.3.1",
		"popper.js": "^1.14.4",
		"reflect-metadata": "^0.1.10",
		"rxjs": "^5.5.6",
		"tslib": "^1.8.1",
		"zone.js": "^0.8.20"
	},
	"devDependencies": {
		"@angular/compiler": "^5.2.0",
		"@angular/compiler-cli": "^5.2.0",
		"@ngtools/webpack": "^1.9.4",
		"@types/core-js": "^0.9.44",
		"@types/node": "^9.3.0",
		"angular-router-loader": "^0.8.2",
		"angular2-template-loader": "^0.6.2",
		"awesome-typescript-loader": "^3.4.1",
		"cross-env": "^5.1.3",
		"css-loader": "^1.0.0",
		"extract-text-webpack-plugin": "^3.0.2",
		"file-loader": "^1.1.6",
		"html-loader": "^0.5.4",
		"html-webpack-plugin": "^3.2.0",
		"node-sass": "^4.7.2",
		"raw-loader": "^0.5.1",
		"rimraf": "^2.6.2",
		"sass-loader": "^6.0.6",
		"style-loader": "^0.23.1",
		"typescript": "^2.6.2",
		"url-loader": "^0.6.2",
		"webpack": "^3.10.0",
		"webpack-dev-server": "^2.11.0",
		"webpack-merge": "^4.1.0"
	}
}
